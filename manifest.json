{
  "manifest_version": 2,

  "name": "Sesame Time",
  "description": "This extension allows the user to track its working time.",
  "version": "1.0.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "main.js"
    ]
  },

  "permissions": [
    "tabs",
    "*://*/*",
    "webNavigation",
    "cookies"
  ]
}
